<?xml version="1.0" encoding="ASCII"?>
<StatechartLanguage:Statechart
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:StatechartLanguage="platform:/plugin/StatechartLanguage/model/StatechartLanguage.ecore">
  <topState
      name="top">
    <contains
        xsi:type="StatechartLanguage:HyperEdge"
        next="//@topState/@contains.1/@contains.0 //@topState/@contains.2/@contains.0"
        rnext="//@topState/@contains.1/@contains.1"
        name="t1"/>
    <contains
        xsi:type="StatechartLanguage:OR"
        name="">
      <contains
          xsi:type="StatechartLanguage:Basic"
          next="//@topState/@contains.1/@contains.2"
          rnext="//@topState/@contains.0"
          name="p3"/>
      <contains
          xsi:type="StatechartLanguage:Basic"
          next="//@topState/@contains.0"
          rnext="//@topState/@contains.1/@contains.2"
          name="p1"/>
      <contains
          xsi:type="StatechartLanguage:HyperEdge"
          next="//@topState/@contains.1/@contains.1"
          rnext="//@topState/@contains.1/@contains.0"
          name="t2"/>
    </contains>
    <contains
        xsi:type="StatechartLanguage:OR"
        name="">
      <contains
          xsi:type="StatechartLanguage:Basic"
          next="//@topState/@contains.2/@contains.2"
          rnext="//@topState/@contains.0"
          name="p2"/>
      <contains
          xsi:type="StatechartLanguage:Basic"
          rnext="//@topState/@contains.2/@contains.2"
          name="p4"/>
      <contains
          xsi:type="StatechartLanguage:HyperEdge"
          next="//@topState/@contains.2/@contains.1"
          rnext="//@topState/@contains.2/@contains.0"
          name="t3"/>
    </contains>
  </topState>
</StatechartLanguage:Statechart>
