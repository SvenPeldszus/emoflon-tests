rule ForAllShelfRule {
	source {
		inputFolder : Folder { 
			++ -subFolder-> shelfFolder
		}
		
		++ shelfFolder : Folder
	}
	
	correspondence {
		inputFolder <- folderToLibrary : FolderToLibrary -> library
	
		++ shelfFolder <- folderToShelf : FolderToShelf -> shelf
	}
	
	target {
		library : Library { 
			++ -shelf-> shelf
		}
		
		++ shelf : Shelf
	
	}
	
	constraints {
		eq(shelfFolder.name, shelf.description)
	}
}